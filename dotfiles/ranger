set show_cursor true

unmap ;
map ;<Right> tab_move 1
map ;<Left>  tab_move -1
map ;1       tab_open 1
map ;2       tab_open 2
map ;3       tab_open 3
map ;4       tab_open 4
map ;5       tab_open 5
map ;6       tab_open 6
map ;7       tab_open 7
map ;8       tab_open 8
map ;9       tab_open 9
map ;0       tab_open 10
map ;r       tab_shift 1
map ;l       tab_shift -1
map ;q       exit

